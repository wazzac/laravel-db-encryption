# Laravel DB Encryption Configuration
# =====================================

# Encryption Key (REQUIRED)
# Generate with: php artisan db-encrypt:generate-key
# NEVER commit this to version control!
DB_ENCRYPT_KEY=

# Logging Level
# 0 = None (production recommended)
# 1 = High-Level (errors only)
# 2 = Mid-Level (warnings)
# 3 = Low-Level (debug, all operations)
DB_ENCRYPT_LOG_LEVEL=0

# Log Indicator/Prefix
# Used to filter logs: tail -f storage/logs/laravel.log | grep db-encrypt
DB_ENCRYPT_LOG_INDICATOR=db-encrypt

# Primary Key Format
# Options: 'int' (default) or 'uuid'
DB_ENCRYPT_DB_PRIMARY_KEY_FORMAT=int
