{
    "name": "wazza/laravel-db-encryption",
    "description": "Production-ready Laravel package for secure, transparent encryption of sensitive model attributes with searchable hash indexes and key rotation support.",
    "type": "library",
    "keywords": [
        "laravel",
        "encryption",
        "database-encryption",
        "security",
        "gdpr",
        "hipaa",
        "pci-dss",
        "aes-256",
        "openssl",
        "eloquent",
        "laravel-12"
    ],
    "homepage": "https://www.wazzac.dev",
    "repository": {
        "type": "git",
        "url": "https://github.com/wazzac/laravel-db-encryption"
    },
    "readme": "README.md",
    "require": {
        "ext-json": "*",
        "ext-openssl": "*",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/console": "^12.0",
        "php": "^8.2 || ^8.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20.0",
        "laravel/legacy-factories": "~1",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "phpunit/phpunit": "^11.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wazza\\DbEncrypt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wazza\\DbEncrypt\\": "src/",
            "Wazza\\DbEncrypt\\Tests\\": "tests/",
            "Wazza\\DbEncrypt\\Database\\Factories\\": "database/factories/",
            "Wazza\\DbEncrypt\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wazza\\DbEncrypt\\Providers\\DbEncryptServiceProvider"
            ],
            "aliases": {
                "DbEncrypt": "Wazza\\DbEncrypt\\Facades\\DbEncrypt"
            }
        }
    },
    "authors": [
        {
            "name": "Warren Coetzee",
            "email": "warren.coetzee@gmail.com",
            "homepage": "https://www.wazzac.dev",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/wazzac/laravel-db-encryption/issues",
        "source": "https://github.com/wazzac/laravel-db-encryption",
        "docs": "https://github.com/wazzac/laravel-db-encryption/blob/main/README.md"
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
